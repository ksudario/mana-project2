<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Name</th>
      <th>Website</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% resources.forEach(function(r) { %>
      <tr>
        <td><%= r.Name %></td>
        <td><%= r.Website %></td>
        <td><a href="/resources/<%= r._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>